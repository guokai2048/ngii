<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewBag.Title</title>

     <!--框架必需start-->
    <script src="~/Content/scripts/jquery/jquery-1.10.2.min.js"></script>
    <link href="~/Content/styles/font-awesome.min.css" rel="stylesheet" />

    <!--框架必需end-->
    <!--bootstrap组件start-->
    <link href="~/Content/scripts/bootstrap/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Content/scripts/bootstrap/bootstrap.min.js"></script>
    <!--bootstrap组件end-->

    <!--easyui组件start-->
    <link href="~/Content/scripts/easyui/jquery-easyui-1.5/themes/bootstrap/easyui.css" rel="stylesheet" />
    <link href="~/Content/scripts/easyui/jquery-easyui-1.5/themes/icon.css" rel="stylesheet" />
    <script src="~/Content/scripts/easyui/jquery-easyui-1.5/jquery.easyui.min.js"></script>
    <script src="~/Content/scripts/easyui/jquery-easyui-1.5/locale/easyui-lang-zh_CN.js"></script>
    <script src="~/Content/scripts/easyui/syUtil.js"></script>
    <script src="~/Content/scripts/utils/mesComm.js"></script>
    <script src="~/Content/scripts/utils/easyuiComm.js"></script> 
    <!--easyui的tree样式-->
    <link href="~/Content/scripts/easyui/Site.css" rel="stylesheet" />
    <!--easyui组件end-->
    <script src="~/Content/scripts/plugins/datepicker/WdatePicker.js"></script>
    <script src="~/Content/scripts/artTemplate/template-web.js"></script>
    
    @System.Web.Optimization.Styles.Render(
    "~/Content/styles/CM-ui.css")
    @System.Web.Optimization.Scripts.Render(
    "~/Content/scripts/utils/js")

</head>
<body>
    @RenderBody()
    @Html.AntiForgeryToken()
</body>
</html>
